{
  "name": "ozone-api-authentication",
  "version": "0.0.1",
  "description": "low level wrapper around ozone login, logout and authentication api",
  "scripts": {
    "start": "webpack --config webpack.config.js",
    "build": "tsc",
    "prepublish": "tsc",
    "demo": "webpack-dev-server --config webpack.config.js",
    "test": "wct --skip-plugin=local --configFile wct.conf.js",
    "test:persist": "wct --skip-plugin=headless -p --configFile wct.conf.js",
    "build:test:watch": "webpack --config webpack.test.js --watch",
    "pretest": "webpack --config webpack.test.js",
    "doc": "typedoc --entryPoint src/index.ts --out  doc/ src/"
  },
  "main": "dist/lib/ozone-api-authentication/ozone-api-authentication.js",
  "types": "dist/lib/ozone-api-authentication/ozone-api-authentication.d.ts",
  "author": "",
  "license": "ISC",
  "dependencies": {
    "global": "^4.3.2",
    "ozone-api-request": "^0.0.1",
    "ozone-config": "^0.0.1",
    "reflect-metadata": "^0.1.10"
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/mocha": "^2.2.43",
    "@types/sinon": "^2.3.6",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "html-webpack-plugin": "^2.30.1",
    "ozone-type": "git+https://git@bitbucket.taktik.be/scm/sandbox/ozone-type.git",
    "polymer-webpack-loader": "^2.0.0",
    "taktik-polymer-typescript": "^0.0.1",
    "ts-loader": "^2.3.7",
    "typedoc": "^0.9.0",
    "typescript": "^2.7.2",
    "wct-headless": "^2.2.0",
    "wct-istanbul": "^0.12.4",
    "web-component-tester": "^6.3.0",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  }
