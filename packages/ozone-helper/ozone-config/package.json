{
  "name": "ozone-config",
  "version": "4.0.0-alpha.1",
  "publishConfig": {
    "access": "public"
  },
  "description": "Ozone config loader",
  "repository": {
    "type": "git",
    "url": "https://github.com/taktik/ozone-components/packages/ozone-helper/ozone-config"
  },
  "main": "dist/src/ozone-config.js",
  "types": "dist/src/ozone-config.d.ts",
  "scripts": {
    "prepublish": "npm run tsc",
    "bower": "bower install",
    "tsc": "tsc",
    "test": "echo $npm_package_name && wct --skip-plugin=local --configFile wct.conf.js",
    "test:persist": "wct --skip-plugin=headless -p --configFile wct.conf.js",
    "build:watch": "webpack --config webpack.test.js --watch",
    "build": "webpack --config webpack.test.js",
    "pretest": "npm run build",
    "doc": "typedoc --excludeExternals --out  doc/ src/"
  },
  "author": "hj@taktik",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "html-webpack-plugin": "^3.0.7",
    "ozone-type": "^3.4.2",
    "polymer-webpack-loader": "^2.0.3",
    "reflect-metadata": "^0.1.12",
    "ts-loader": "^4.5.0",
    "typescript": "^2.9.2",
    "wct-headless": "^2.2.2",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "@types/lodash": "^4.14.116",
    "ozone-api-request": "^4.0.0-alpha.1",
    "taktik-polymer-typescript": "^4.0.0-alpha.1",
    "typedoc": "^0.11.1",
    "web-component-tester": "^6.8.0"
  }
}
